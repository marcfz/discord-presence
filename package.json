{
  "name": "discord-presence",
  "version": "1.0.0",
  "description": "Visual Studio Code extension for Discord Presence.",
  "main": "./build/extension",
  "scripts": {
    "lint": "eslint ."
  },
  "contributes": {
    "commands": [
      {
        "command": "discord.enable",
        "title": "Enable Discord Presence"
      },
      {
        "command": "discord.disable",
        "title": "Disable Discord Presence"
      }
    ]
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unrealv1/discord-presence.git"
  },
  "author": "unrealv1",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unrealv1/discord-presence/issues"
  },
  "homepage": "https://github.com/unrealv1/discord-presence#readme",
  "dependencies": {
    "discord-rpc": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-marine": "^9.3.2",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2"
  },
  "engines": {
    "vscode": "^1.64.0"
  }
}
